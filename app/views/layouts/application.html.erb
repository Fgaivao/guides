<!DOCTYPE html>
<html>
  <head>
    <title>Guides</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>


  </head>
  <nav>

<ul>

  <li class="logo">
    LOGO
  </li>
<% if guide_signed_in? %>
  <li class="log">
  <%= link_to('Logout', destroy_guide_session_path, method: :delete) %>
  </li>
<% else %>
  <li class="log">
  <%= link_to('Login', new_guide_session_path) %>
  </li>
<% end %>

</ul>

</nav>

  <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

  <body>
    <%= yield %>
  </body>
</html>
